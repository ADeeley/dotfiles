	// }
	"Print to console": {
		"prefix": "cl",
		"body": [
			"console.log($1);"
		],
		"description": "Log output to console"
	},
	"Print to console table": {
		"prefix": "ct",
		"body": [
			"console.table($1);"
		],
		"description": "Log output to console in table format"
	},
	"Set Interval": {
		"prefix": "sint",
		"body": [
			"setInterval(($1) => {",
			"",
			"}, $2);"
		],
		"description": "Snippet for set interval"
	},
	"Set Timeout": {
		"prefix": "stim",
		"body": [
			"setTimeout(($1) => {",
			"",
			"}, $2);"
		],
		"description": "Snippet for set timeout"
	},
	"Const element grab": {
		"prefix": "cqs",
